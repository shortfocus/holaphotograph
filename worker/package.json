{
  "name": "holaphotograph-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create holaphotograph-db",
    "db:migrate": "wrangler d1 migrations apply holaphotograph-db",
    "db:migrate:remote": "wrangler d1 migrations apply holaphotograph-db --remote",
    "db:local": "wrangler d1 execute holaphotograph-db --local --file=./migrations/0001_init.sql"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250210.0",
    "@types/sanitize-html": "^2.16.0",
    "typescript": "^5.7.3",
    "wrangler": "^4.66.0"
  },
  "dependencies": {
    "sanitize-html": "^2.17.1"
  }
}
